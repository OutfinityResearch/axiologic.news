{
  "apiKeys": {
    "openai": "$OPENAI_API_KEY",
    "anthropic": "$ANTHROPIC_API_KEY",
    "claude": "$CLAUDE_API_KEY",
    "mistral": "$MISTRAL_API_KEY",
    "gemini": "$GEMINI_API_KEY",
    "groq": "$GROQ_API_KEY"
  },
  "defaultProvider": "mistral",
  "defaultModels": {
    "openai": "gpt-4o-mini",
    "anthropic": "claude-3-haiku-20240307",
    "claude": "claude-3-haiku-20240307",
    "mistral": "mistral-small-latest",
    "gemini": "gemini-pro",
    "groq": "mixtral-8x7b-32768",
    "ollama": "llama2"
  },
  "ollamaHost": "http://localhost:11434",
  "promotionalBanner": {
    "enabled": true,
    "defaultText": "Axiologic News",
    "defaultUrl": "https://axiologic.news",
    "customBanners": {
      "tech": {
        "text": "Tech News by Axiologic.News",
        "url": "https://axiologic.news/"
      },
      "ai": {
        "text": "AI Insights by Axiologic.News",
        "url": "https://axiologic.news/"
      },
      "startups": {
        "text": "Startup Stories by Axiologic.News",
        "url": "https://axiologic.news/"
      },
      "science": {
        "text": "Science Discoveries by Axiologic.News",
        "url": "https://axiologic.news/"
      },
      "world": {
        "text": "World News by Axiologic.News",
        "url": "https://axiologic.news/"
      }
    }
  },
  "contentSettings": {
    "maxPostsPerFeed": 50,
    "maxTokensPerRequest": 500,
    "contentFetchTimeout": 10000,
    "requestTimeout": 30000,
    "historyDays": 3
  },
  "debug": false
}